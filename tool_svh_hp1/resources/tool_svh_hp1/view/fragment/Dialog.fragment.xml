<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Tables"
		class="sapUiPopupWithPadding"
		items="{/TableSet}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{TABLEID}"
			description="{DDTEXT}" />
	</SelectDialog>
</core:FragmentDefinition>