<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="{i18n>assetno}"
		ok=".onValueHelpOkPressAssetNo"
		cancel=".onValueHelpCancelPressAssetNo"
		afterClose=".onValueHelpAfterCloseAssetNo"
		supportRanges="true"
		key="ANLN1"
		descriptionKey="ANLN1"
		supportMultiselect="false"
	>
		<filterBar>
			<fb:FilterBar advancedMode="true" search="onFilterBarSearchAssetNo">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="MANDT"
						label="{i18n>client}"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="MANDT" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="BUKRS"
						label="{i18n>companycode}"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="BUKRS" />
						</fb:control>
					</fb:FilterGroupItem>

					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="ANLN1"
						label="{i18n>assetno}"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="ANLN1" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
