<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<Dialog title="{i18n>filename}">
		<content>
			<Panel headerText="Enter Filename" class="sapUiResponsiveMargin" width="auto">
				<content>			
					<Input id="filename" value="Export.xls" placeholder="{i18n>filename}"/>

				</content>
			</Panel>									
		</content>
		<beginButton>
			<Button text="OK" press="onPressOkFilename"></Button>
		</beginButton>
		<endButton>
			<Button text="Close" press="onPressCloseFilename"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>