service  { 

"CS.UiPersonalizationTable" as "fav" 
create using "xsjs:usersMethod.xsjslib::usersCreate";
}
settings {  
  support null;
  content cache-control "no-store"; 
  metadata cache-control "max-age=86401,must-revalidate";   
  limits 
      max_records = 500000,
      max_expanded_records = 500000;
}