{
	"welcomeFile": "index.html",
	"authenticationMethod": "route",
	"routes": [{
		"source": "(.*)(.xsjs)",
		"destination": "xsjs_api",
		"csrfProtection": false,
		"authenticationType": "xsuaa"

	}, 
	{
		"source": "(.*)(.xsodata)",
		"destination": "xsjs_api",
		"authenticationType": "xsuaa"
	},
	{
		"source": "^/(.*)$",
		"localDir": "resources/tool_vin_ep1",
		"authenticationType": "xsuaa",
		"scope": {
			"GET": "$XSAPPNAME.Display_EP1",
			"default": "$XSAPPNAME.Display_EP1"
		}		

	}]
}