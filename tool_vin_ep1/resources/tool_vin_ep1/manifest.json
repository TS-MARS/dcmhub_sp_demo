{
	"_version": "1.17.0",
	"sap.app": {
		"id": "tool_vin_ep1",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.38.11"
		},
		"crossNavigation": {
					"inbounds": {
						"tool_vin_ep1Show": {
							"title": "Decommision Hub",
							"icon": "sap-icon://share-2",
							"semanticObject": "tool_vin_ep1",
							"action": "show",
							"deviceTypes": {
								"desktop": true,
								"tablet": true,
								"phone": true
							}
						}
					},
					"outbounds": {}
				},		
		"dataSources": {
			"navigationService": {
				"preload": true,
				"uri": "../xsodata/application.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": [
						"annotation"
					]
				}
			},
			"toolTableService": {
				"uri": "../xsodata/tooltableservice.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": [
						"annotation"
					]
				}
			},
				"personalization": {
				"uri": "../xsodata/fav.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"attach": {
				"uri": "../xsodata/attach.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"obj": {
				"uri": "../xsodata/obj.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},				
				"searchhelp": {
				"uri": "../xsodata/searchhelp.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"fullWidth":true,
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
		
	},
	"sap.ui5": {
	"componentName": "tool_vin_ep1",		
		"flexEnabled": false,
		"rootView": {
			"viewName": "tool_vin_ep1.view.launchpad",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"minUI5Version": "1.71.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {},
				"sap.tnt" :{},
				"sap.ui.table":{},
				"sap.ui.comp":{}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": false
		},
		"models": {
			"navigationSet": {
				"type" : "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "navigationService",
				"defaultBindingMode": "TwoWay",
				"preload": true
			},
			"toolTableServiceSet": {
				"type" : "sap.ui.model.odata.v2.ODataModel",				
				"dataSource": "toolTableService",
				"defaultBindingMode": "TwoWay",
				"preload": true
			},				
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "tool_vin_ep1.i18n.i18n"
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"attachSet": {
				"type" : "sap.ui.model.odata.v2.ODataModel",				
				"dataSource": "attach",
				"defaultBindingMode": "TwoWay",
				"preload": true
			},
			"objSet": {
				"type" : "sap.ui.model.odata.v2.ODataModel",				
				"dataSource": "obj",
				"defaultBindingMode": "TwoWay",
				"preload": true
			},				
			"persModel": {
				"type" : "sap.ui.model.odata.v2.ODataModel",				
				"dataSource": "personalization",
				"preload": true,
				"settings":{
					"defaultBindingMode": "TwoWay",
					"json": true,
					"useBatch":false,
					"defaultUpdateMethod": "PUT"
				}
			},
			"searchModel": {
				"type" : "sap.ui.model.odata.v2.ODataModel",				
				"dataSource": "searchhelp",
				"preload": true,
				"settings":{
					"defaultBindingMode": "TwoWay",
					"json": true
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css?portalInterceptorAppId=tool_vin_ep1"
				}
			],
			"js": [
				{
					"uri": "util/xlsx.full.min.js"
				}
			]
		},
		"routing": {
			"config": {
				"fullWidth": true,
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "tool_vin_ep1.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "launchpad",
					"pattern": "launchpad",
					"target": [
						"launchpad"
					]
				},
				{
					"name": "reporting",
					"pattern": "reporting",
					"target": [
						"reporting"
					]
				},
				{
					"name": "adhocQuery",
					"pattern": "adhocQuery",
					"target": [
						"adhocQuery"
					]
				},
				{
					"name": "attachment",
					"pattern": "attachment",
					"target": [
						"attachment"
					]
				}				
			],
			"targets": {
				"launchpad": {
					"viewType": "XML",
					"transition": "fade",
					"clearControlAggregation": false,
					"viewId": "launchpad",
					"viewName": "launchpad"
				},
				"reporting": {
					"viewType": "XML",
					"transition": "fade",
					"clearControlAggregation": false,
					"viewId": "reporting",
					"viewName": "reporting"
				},
				"adhocQuery": {
					"viewType": "XML",
					"transition": "fade",
					"clearControlAggregation": false,
					"viewId": "adhocQuery",
					"viewName": "adhocQuery"
				},
				"attachment": {
					"viewType": "XML",
					"transition": "fade",
					"clearControlAggregation": false,
					"viewId": "attachment",
					"viewName": "Attachment"
				}				
			}
		},
		"config": {
                "sapFiori2Adaptation": true
                }
	},
	"sap.platform.cf": {
      "oAuthScopes": ["$XSAPPNAME.Display_EP1"]	
	},			
	"sap.platform.runtime": {
	"componentProperties": {
			"url": "/"
		}
	}	
}