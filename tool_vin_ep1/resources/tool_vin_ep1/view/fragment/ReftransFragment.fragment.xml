<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="{i18n>reftrans}"
		ok=".onValueHelpOkPressReftrans"
		cancel=".onValueHelpCancelPressReftrans"
		afterClose=".onValueHelpAfterCloseReftrans"
		supportRanges="true"
		key="AWTYP"
		descriptionKey="AWTYP"
		supportMultiselect="true"
	>
		<filterBar>
			<fb:FilterBar advancedMode="true" search="onFilterBarSearchReftrans">
				<fb:filterGroupItems>
					
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="AWTYP"
						label="{i18n>Reftrans}"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="AWTYP" />
						</fb:control>
					</fb:FilterGroupItem>

				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
