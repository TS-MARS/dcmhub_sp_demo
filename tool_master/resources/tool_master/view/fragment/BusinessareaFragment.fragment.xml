<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="{i18n>barea}"
		ok=".onValueHelpOkPressBarea"
		cancel=".onValueHelpCancelPressBarea"
		afterClose=".onValueHelpAfterCloseBarea"
		supportRanges="true"
		key="GSBER"
		descriptionKey="GSBER"
			supportMultiselect="false"
	>
		<filterBar>
			<fb:FilterBar advancedMode="true" search="onFilterBarSearchBarea">
				<fb:filterGroupItems>
					
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="MANDT"
						label="{i18n>client}"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="MANDT" />
						</fb:control>
					</fb:FilterGroupItem>
					
						<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="VERSN"
						label="{i18n>barea}"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="GSBER" />
						</fb:control>
					</fb:FilterGroupItem>

				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
